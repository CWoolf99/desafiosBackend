<div class="text-bg-info p-3">
    <h1>Productos</h1>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">Nombre</th>
                <th scope="col">Precio</th>
                <th scope="col">Imagen</th>
            </tr>
        </thead>
        <tbody>
        {{#each prods}}
        <tr>
            <td>{{this.nombre}}</td>
            <td>{{this.precio}}</td>
            <td>
                <img src={{this.imagen}} class="img-thumbnail img-fluid" height="150" width="150">
            </td>
        </tr>
        {{/each}}
        </tbody>
    </table>
</div>